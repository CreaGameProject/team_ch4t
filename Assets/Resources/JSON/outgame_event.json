      "type": "END",
      "event_id": 3,
      "name": null,
      "file": null,
      "name_sub": null,
      "file_sub": null,
      "talker": null,
      "text": null
    },
    {
      "event_number": 97,
      "type": "FADE_IN",
      "event_id": 4,
      "name": null,
      "file": "pict_backGround",
      "name_sub": null,
      "file_sub": null,
